{
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-ie": "^6.7.0",
    "define": "^1.0.1",
    "dijit": "^1.13.0",
    "dojo": "^1.13.0",
    "jimu": "^0.0.94",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  },
  "scripts": {
    "build:widgetjs": "babel --config ./.babelrc ./Widget.js --out-file dist/Widget.js",
    "build:settingsjs": "babel --config ./.babelrc ./setting/setting.js --out-file dist/setting.js",
    "build:xml2json": "babel --config ./.babelrc ./setting/xml2json.js --out-file dist/xml2json.js",
    "build:sldstyling": "babel --config ./.babelrc ./js/sldStyling.js --out-file dist/sldStyling.js",
    "watch": "yarn run build --watch",
    "build": "yarn run build:widgetjs && yarn run build:settingsjs && yarn run build:xml2json && yarn run build:sldstyling"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "normalize.css": "^8.0.0"
  }
}
